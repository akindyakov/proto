defaults
tls on
tls_starttls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile ~/tmp/msmtp.log

account gmail
host smtp.gmail.com
port 587
protocol smtp
auth on
from akindyakov@gmail.com
user akindyakov@gmail.com
passwordeval "pass google.com/akindyakov@gmail.com"

account magictwin
host smtp.yandex.com
port 587
protocol smtp
auth on
from magictwin@yandex.ru
user magictwin@yandex.ru
passwordeval "pass yandex.ru/magictwin@yandex.ru"
